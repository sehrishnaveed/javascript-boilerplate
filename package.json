{
  "name": "javascript-boilerplate",
  "version": "1.0.0",
  "description": "JavaScript multipage boilerplate using webpack 5",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --mode=development & webpack --mode development",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "prettier . --check"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sehrishnaveed/javascript-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sehrishnaveed/javascript-boilerplate/issues"
  },
  "homepage": "https://github.com/sehrishnaveed/javascript-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.48.0",
    "eslint-webpack-plugin": "^4.0.1",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "^9.0.0",
    "prettier": "^3.0.3",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
